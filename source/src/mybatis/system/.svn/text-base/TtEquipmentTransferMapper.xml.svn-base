  <!-- sf 通过设备ID查找 -->
  <select id="findeByeqPK" resultType="pd" parameterType="pd" >
    SELECT 
 IFNULL((SELECT t.`ITEM_NAME` FROM tt_iteminfo  t  WHERE t.`ITEM_ID`=t.TRANSFER_IN_ITEM_ID),'公司') AS TRANSFER_IN_ITEM_ID, 
    (SUM(TRANSFER_NUMBER)) as TRANSFER_NUMBER
