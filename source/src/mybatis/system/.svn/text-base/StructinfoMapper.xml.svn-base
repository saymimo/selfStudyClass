      t.`CURRENT_STRUCT_ID` 
    FROM
      `tt_item_struct` t 
    WHERE t.`STRUCT_ID` =#{STRUCT_ID3} 
      OR t.`CURRENT_STRUCT_ID` = #{STRUCT_ID3} ) 
       OR s.`STRUCT_ID`= #{STRUCT_ID3}
       OR s.`STRUCT_ID` =(SELECT t.`STRUCT_ID` FROM `sys_structinfo` t WHERE t.`STRUCTST_AGE`=0)
  </select>
  
