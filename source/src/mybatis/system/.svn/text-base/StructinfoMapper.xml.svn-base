  <select id="findListById" parameterType="pd" resultType="pd" useCache="false">
  	SELECT 
  * 
FROM
  `sys_structinfo` s 
WHERE s.`STRUCT_ID` IN 
  (SELECT 
    t.`STRUCT_ID` 
