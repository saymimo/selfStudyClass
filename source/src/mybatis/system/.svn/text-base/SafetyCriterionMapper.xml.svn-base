SELECT 
  c.*,
  (SELECT 
    s.STRUCTST_AGE 
  FROM
    sys_structinfo s 
  WHERE s.STRUCT_ID = c.`STRUCT_ID`) AGE 
FROM
  tt_safaty_criterion c 
