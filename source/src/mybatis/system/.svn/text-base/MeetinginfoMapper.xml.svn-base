  </update>
  <!-- 批量删除用户 -->
	<delete id="deleteAllM" parameterType="String" flushCache="false">
	 update tt_meetinginfo set   IS_DEL = 1
		where 
			MEETINGINFO_ID in
  			 <foreach item="item" index="index" collection="array" open="(" separator="," close=")">
                  #{item}
             </foreach>
	</delete>
	
	
	 <!-- 查询已选人员 -->
