<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="TtPactinfoMapper">
	
	
	<!-- 新增-->
	<insert id="save" parameterType="pd">
		insert into tt_pactinfo(
			PACT_ID
			,ITEM_ID
			,PACT_NUMBER
			,PACT_NAME
			,PACT_TYPE
			,OTHER_UNIT
			,MARK_MONEY
			,MAKE_DATE
			,SWORK_DATE
			,EWORK_DATE
			,PACT_DATE
			,PACT_MARK
			,PACT_AMOUNT
			,PACT_PRICE
			,PACT_UNIT
			,PACT_ADDRESS
			,REMARK
			,FILE_PATH
			,FILE_NAME
			,IS_DEL
			,CREATE_DATE
			,CREATE_BY
			,UPDATE_DATE
			,UPDATE_BY
			,REVIEW_TYPE
			,FLOW_STATE
			,REVIEW_STATE
			,IDENTIFY_NUMBER
			,OTHER_ADDRESS
			,OTHER_PHONE
			,OTHER_NUMBER
		) values (
			 #{PACT_ID}
			,#{ITEM_ID}
			,#{PACT_NUMBER}
			,#{PACT_NAME}
			,#{PACT_TYPE}
			,#{OTHER_UNIT}
			,#{MARK_MONEY}
			,#{MAKE_DATE}
			,#{SWORK_DATE}
			,#{EWORK_DATE}
			,#{PACT_DATE}
			,#{PACT_MARK}
			,#{PACT_AMOUNT}
			,#{PACT_PRICE}
			,#{PACT_UNIT}
			,#{PACT_ADDRESS}
			,#{REMARK}
			,#{FILE_PATH}
			,#{FILE_NAME}
			,#{IS_DEL}
			,#{CREATE_DATE}
			,#{CREATE_BY}
			,#{UPDATE_DATE}
			,#{UPDATE_BY}
			,#{REVIEW_TYPE}
			,#{FLOW_STATE}
			,#{REVIEW_STATE}
			,#{IDENTIFY_NUMBER}
			,#{OTHER_ADDRESS}
			,#{OTHER_PHONE}
			,#{OTHER_NUMBER}
		)
	</insert>
	
	
	<!-- 删除-->
	<delete id="delete" parameterType="pd">
		DELETE FROM tt_pactinfo
		WHERE 
			PACT_ID = #{PACT_ID}
	</delete>
	
	
	<!-- 修改 -->
	<update id="edit" parameterType="pd">
		update  tt_pactinfo
			set 
				
				
				PACT_NUMBER     =          #{PACT_NUMBER},
				PACT_NAME       =          #{PACT_NAME},
				PACT_TYPE       =          #{PACT_TYPE},
				OTHER_UNIT      =          #{OTHER_UNIT},
				MARK_MONEY      =          #{MARK_MONEY},
				MAKE_DATE       =          #{MAKE_DATE},
				SWORK_DATE      =          #{SWORK_DATE},
				EWORK_DATE      =          #{EWORK_DATE},
				PACT_DATE       =          #{PACT_DATE},
				PACT_MARK       =          #{PACT_MARK},
				PACT_AMOUNT     =          #{PACT_AMOUNT},
				PACT_PRICE      =          #{PACT_PRICE},
				PACT_UNIT       =          #{PACT_UNIT},
				PACT_ADDRESS    =          #{PACT_ADDRESS},
				REMARK         =           #{REMARK},
				UPDATE_DATE     =          #{UPDATE_DATE},
				UPDATE_BY       =          #{UPDATE_BY},
				REVIEW_TYPE     =          #{REVIEW_TYPE},
				OTHER_NUMBER         =     #{OTHER_NUMBER},
				IDENTIFY_NUMBER     =      #{IDENTIFY_NUMBER},
				OTHER_ADDRESS       =      #{OTHER_ADDRESS},
				OTHER_PHONE     =          #{OTHER_PHONE},
				REVIEW_STATE     =         #{REVIEW_STATE}
			
			where 
				PACT_ID = #{PACT_ID}
	</update>
	
	
	<!-- 通过ID获取数据 -->
	<select id="findById" parameterType="pd" resultType="pd">
		select 
			PACT_ID       
			,ITEM_ID      
			,PACT_NUMBER  
			,PACT_NAME    
			,PACT_TYPE    
			,OTHER_UNIT   
			,MARK_MONEY   
			,MAKE_DATE    
			,SWORK_DATE   
			,EWORK_DATE   
			,PACT_DATE    
			,PACT_MARK    
			,PACT_AMOUNT  
			,PACT_PRICE   
			,PACT_UNIT    
			,PACT_ADDRESS 
			,REMARK      
			,FILE_PATH    
			,FILE_NAME    
			,IS_DEL       
			,CREATE_DATE  
			,CREATE_BY    
			,UPDATE_DATE  
			,UPDATE_BY 
			,IDENTIFY_NUMBER
			,OTHER_ADDRESS
			,OTHER_PHONE
			,OTHER_NUMBER   
			,(SELECT ITEM_REVIEW_ID FROM `tt_item_review` t WHERE tt_pactinfo.`PACT_ID`=t.FOREIGN_KEY_ID AND TYPE=2) AS ITEM_REVIEW_ID				
		from 
			tt_pactinfo
		where 
			PACT_ID = #{PACT_ID} and IS_DEL=0
	</select>
<select id="pactTypes" resultType="pd">
SELECT 
  s.`NAME`,
  s.`BIANMA` 
FROM
  sys_dictionaries s 
WHERE s.`P_BM` LIKE 'HTLX_%' 
</select>	
