  <!-- 税票管理主页查询  子-->
  <select id="listPageAllTax" resultType="pd" parameterType="page"  useCache="false">
  SELECT 
  (SELECT 
    NAME 
  FROM
    `sys_user` u 
  WHERE t.`CREATE_BY` = u.USER_ID) AS CREATE_NAME,
  t.`CREATE_DATE`,
  t.`FAIL_REMARK`,
  t.`FINANCE_NO`,
  t.`INVOICE_CODE`,
  t.`INVOICE_MONEY`,
  t.`INVOICE_NO`,
  t.`INVOICE_TAG`,
 (SELECT i.`ITEM_NAME` FROM`tt_iteminfo` i WHERE i.`ITEM_ID`=t.`ITEM_ID`)AS ITEM_NAME,
  t.`SALES_NAME`,
  t.`STRUCT_ID`,
  t.`STRUCT_NO`,
  t.`TAX_DATE`,
  t.`TAX_MOENY`,
  t.`TAX_NO`,
  t.`TAX_STAMP_ID`,
  t.`TAX_STATE`,
  t.`UP_DATE`,
  (SELECT 
    n.STRUCT_NAME 
  FROM
    `sys_structinfo` n 
  WHERE n.STRUCT_ID = t.`STRUCT_ID`) AS NAME 
FROM
  `tt_tax_stamp` t 
WHERE STRUCT_ID  =#{pd.STRUCT_ID}
	 <if test="pd.NAME != null and pd.NAME != ''">
			and	 INVOICE_CODE LIKE CONCAT(CONCAT('%', #{pd.NAME}),'%')
		</if><!-- 关键词检索 -->
ORDER BY  CREATE_DATE DESC
  </select>
  
   <!-- 税票管理主页查询 区域 -->
  <select id="listPageOneTax" resultType="pd" parameterType="page"  useCache="false">
   SELECT 
  (SELECT 
    NAME 
  FROM
    `sys_user` u 
  WHERE t.`CREATE_BY` = u.USER_ID) AS CREATE_NAME,
  t.`CREATE_DATE`,
  t.`FAIL_REMARK`,
  t.`FINANCE_NO`,
  t.`INVOICE_CODE`,
  t.`INVOICE_MONEY`,
  t.`INVOICE_NO`,
  t.`INVOICE_TAG`,
 (SELECT i.`ITEM_NAME` FROM`tt_iteminfo` i WHERE i.`ITEM_ID`=t.`ITEM_ID`)AS ITEM_NAME,
  t.`SALES_NAME`,
  t.`STRUCT_ID`,
  t.`STRUCT_NO`,
  t.`TAX_DATE`,
  t.`TAX_MOENY`,
  t.`TAX_NO`,
  t.`TAX_STAMP_ID`,
  t.`TAX_STATE`,
  t.`UP_DATE`,
  (SELECT 
    n.STRUCT_NAME 
  FROM
    `sys_structinfo` n 
  WHERE n.STRUCT_ID = t.`STRUCT_ID`) AS NAME,
  (SELECT 
    n.STRUCTST_AGE 
  FROM
    `sys_structinfo` n 
  WHERE n.STRUCT_ID = t.`STRUCT_ID`) AS AGE 
FROM
  `tt_tax_stamp` t 
 WHERE 
t.`STRUCT_ID`

IN(
SELECT tt.`STRUCT_ID` FROM tt_iteminfo tt WHERE tt.`ITEM_ID` IN (
SELECT i.`ITEM_ID` FROM `tt_item_struct` i WHERE i.`STRUCT_ID`=#{pd.STRUCT_ID})) 
	 <if test="pd.NAME != null and pd.NAME != ''">
			and	 INVOICE_CODE LIKE CONCAT(CONCAT('%', #{pd.NAME}),'%')
		</if><!-- 关键词检索 -->
ORDER BY  CREATE_DATE DESC
  </select>
  
